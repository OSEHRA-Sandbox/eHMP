{
    "apiVersion": "1.0.0",
    "swaggerVersion": "1.3.0",
    "basePath":"http://localhost:9080",
    "resourcePath": "/jds",
    "apis": [
        {
            "path": "/ping",
            "description": "Fetch a patient data item by UID",
            "operations": [
                {
                    "method":"GET",
                    "nickname":"jdsPing",
                    "summary": "Ping the JDS"
                }
            ]
        },
        {
            "path": "/version",
            "operations": [
                {
                    "method":"GET",
                    "nickname":"jdsVersion",
                    "summary": "Fetch the version of the JDS"
                }
            ]
        },
        {
            "path": "/jds/logger/this",
            "operations": [
                {
                    "method":"GET",
                    "nickname":"getLogLevel",
                    "summary": "Get the current JDS log level"
                },
                {
                    "method":"PUT",
                    "nickname":"setLogLevel",
                    "summary": "Set the current JDS log level"
                },
                {
                    "method":"POST",
                    "nickname":"setLogLevel",
                    "summary": "Set the current JDS log level"
                }
            ]
        }
    ]
}
