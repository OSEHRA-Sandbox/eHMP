[{
    "apiVersion": "1.0",
    "description": "Instantiate this activity whenever an update 1) is of domain order, 2) is of subDomain consult 3) was entered in the last 24hr.",
    "eventMatchAction": {
        "startProcessAction": {
            "activityDefinitionId": "Activity.Order",
            "activityDeploymentId": "VistaCore:Activity"
        }
    },
    "eventMatchCriteria": {
        "complexMatch": {
            "booleanOperator": "AND",
            "matchSet": [
                "@.domain == 'order'",
                "@.subDomain == 'consult'",
                "@.visit.dateTime >= 'ONE_DAY_AGO'"
            ]
        },
        "matchType": "JsonPath_v1"
    },
    "matchId": "2e5c1aae-1873-40e6-883f-dfc311ddc19b",
    "name": "New Consult Order Activity Listener"
}]
