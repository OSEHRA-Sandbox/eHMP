{
  "name": "node-vistaexpress",
  "version": "0.0.1",
  "description": "Combination of the VistA Core resource server, resources, and RDK",
  "main": "bin/rdk-fetch-server.js",
  "scripts": {
    "check": "jshint --verbose --config .jshintrc.gradle src",
    "test": "npm run check && npm run test:unit",
    "test:unit": "multi='spec=- xunit=xunit.xml' istanbul cover -x '**/*-spec.js' -x 'test-helpers/*' --report lcov --report cobertura _mocha -- --reporter mocha-multi --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/oracledb-helper.js $(find . -type f -name '*-spec.js' ! -name '*-itest-spec.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*')",
    "test:unit:watch": "mocha --watch --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/oracledb-helper.js $(find . -type f -name '*-spec.js' ! -name '*-itest-spec.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*')",
    "test:integration:fetch": "[[ -z $TEST_CONFIG ]] && export TEST_CONFIG=./config/rdk-fetch-server-config.json ;  mocha --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/environment-helper.js ./test-helpers/oracledb-helper.js $(find . -type f -name '*-itest-spec.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*' ! -path './src/write/*')",
    "test:integration:write-back": "[[ -z $TEST_CONFIG ]] && export TEST_CONFIG=./config/rdk-write-server-config.json ; mocha --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/environment-helper.js ./test-helpers/oracledb-helper.js $(find ./src/write -type f -name '*-itest-spec.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*' ! -path './src/write/pick-list/*')",
    "test:integration:pick-list": "[[ -z $TEST_CONFIG ]] && export TEST_CONFIG=./config/rdk-pick-list-server-config.json ; mocha --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/environment-helper.js ./test-helpers/oracledb-helper.js $(find ./src/write/pick-list -type f -name '*-itest-spec.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*')",
    "test:integration": "echo 'Did you mean one of:\n * test:integration:fetch\n * test:integration:write-back\n * test:integration:pick-list'",
    "test:versioning": "mocha --require ./test-helpers/mocha-helper.js ./test-helpers/sinon-helper.js ./test-helpers/oracledb-helper.js $(find . -type f -name 'versioning-tests.js' ! -path '*/node_modules/*' ! -path '*/_example/*' ! -path './jasmine-tests/*')",
    "check-publish-dates-of-installed-packages": "npm ls --depth=0 --json | jq -r '.dependencies | to_entries | map(\"\\(.key) \\(.value.version)\") | .[]' | while read line; do name=\"${line/ */}\"; version=\"${line/* /}\"; npm view --json $name@$version time | (jq -r --arg version $version '.[$version]'; echo $name) | paste - -; done | sort"
  },
  "dependencies": {
    "agenda": "^0.8.1",
    "aglio-theme-olio": "1.6.10",
    "ajv": "^1.4.10",
    "async": "~2.0.1",
    "body-parser": "^1.12.0",
    "bunyan": "~1.4.0",
    "cli-color": "~0.3.2",
    "cors": "^2.3.1",
    "csrf": "^3.0.1",
    "drafter.js": "2.5.0",
    "express": "4.13",
    "express-session": "~1.13.0",
    "fivebeans": "^1.3.1",
    "forever-agent": "0.6.1",
    "handlebars": "^4.0.5",
    "helmet": "1.3.0",
    "hercule": "^1.4.0",
    "jds-filter": "~1.1.7",
    "jison": "^0.4.15",
    "job-framework": "1.2.9",
    "js-yaml": "^3.7.0",
    "jshint": "2.5.10",
    "json-mask": "^0.3.4",
    "json-stable-stringify": "~1.0.0",
    "jsonwebtoken": "^5.5.4",
    "lodash": "^3.10.1",
    "lzma": "^2.1.1",
    "mathjs": "~1.0.1",
    "memory-cache": "0.0.5",
    "method-override": "^2.3.7",
    "mkdirp": "^0.5.1",
    "moment": "2.15.2",
    "mongodb": "2.2.10",
    "mongoskin": "^2.1.0",
    "morgan": "1.7.X",
    "namecase": "^1.1.0",
    "nconf": "~0.8.4",
    "nedb": "1.5.1",
    "nock": "~2.10.0",
    "node-cache": "~1.0.2",
    "node-mocks-http": "~1.5.0",
    "node-rules": "^3.0.2",
    "node-schedule": "^0.2.8",
    "node-uuid": "~1.4.7",
    "nodemailer": "^2.7.2",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.1",
    "performance-now": "^0.2.0",
    "request": "2.74.0",
    "response-time": "2.3.1",
    "solr-smart-client": "2.0.x",
    "string": "1.9.1",
    "underscore": "~1.6.0",
    "underscore.string": "3.3.4",
    "vista-js": "1.4.3",
    "xml2js": "0.4.17",
    "yargs": "4.3.2"
  },
  "devDependencies": {
    "apidoc-core": "^0.7.1",
    "istanbul": "^0.4.4",
    "mocha": "^2.4.5",
    "mocha-multi": "^0.9.0",
    "must": ">=0.13.1",
    "randexp": "^0.4.2",
    "sinon": "~2.0.0-pre.2",
    "solr-client": "^0.5.0"
  },
  "author": "Steven.Reich",
  "contributors": [
    "Steven.Reich <Steven.Reich@agilex.com>",
    "Jason Ray <jason.ray@agilex.com> (https://github.com/jasonray)"
  ]
}
