        <id>org.healthonnet.lucene:hon-lucene-synonyms</id>
        <phase>initialize</phase>
        <goals>
            <goal>install-file</goal>
        </goals>
        <configuration>
            <pomFile>${basedir}/repo/org/healthonnet/lucene/hon-lucene-synonyms/1.3.3-solr-4.3.0/hon-lucene-synonyms-1.3.3-solr-4.3.0.pom</pomFile>
